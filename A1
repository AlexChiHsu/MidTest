const integer: number[] = [545, -845, 9523, 0, -15]

let max = integer[0]
let min = integer[0]

for(let i = 0; i < 5; i++) {
    
    if(max < integer[i]) {
        max = integer[i]
    }

    if(min >= integer[i]) {
        min = integer[i]
    }
    
}
console.log("最大值: " + max)
console.log("最小值: " + min)
