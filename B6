let output = ""
let cul = "\n"
for(let i = 1; i < 10; i++) {
    for(let j = 2; j < 6; j++) {
        if(i * j < 10) {
            output += j + "*" + i + "=" + " " + (i*j) + " |"            
        }
        else {
            output += j + "*" + i + "=" + " " + (i*j) + "|"
        }
    }
    output += cul
}

console.log("\n" + output)
