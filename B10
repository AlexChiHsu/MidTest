for(var i = 0; i < 7; i++) {
    var buffer = ""
    for(var j = 0; j < 7; j++) {
        if(
            ((i >= 2 && i <=4) && (j >=2 && j <=4) && (i != 3 || j != 3))
        ) {
            buffer += " "
        }
        else {
            buffer += "*"
        }
    } 
     console.log(buffer)
}
