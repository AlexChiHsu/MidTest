for(var i = 0; i < 7; i++) {
    var buffer = ""
    for(var j = 0; j < 7; j++) {
        if(
            ((i == 0 || i == 6) && (j == 1 || j == 5)) ||
            ((i == 1 || i == 5) && (j == 0 || j == 1 || j == 5 || j == 6))
        ) {
            buffer += " "
        }
        else {
            buffer += "*"
        }
    } 
     console.log(buffer)
}
